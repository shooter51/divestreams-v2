{
  "consumer": {
    "name": "DiveStreamsFrontend"
  },
  "interactions": [
    {
      "description": "a request for health status",
      "providerStates": [
        {
          "name": "all services are healthy"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/api/health"
      },
      "response": {
        "body": {
          "checks": {
            "database": "ok",
            "redis": "ok"
          },
          "status": "ok",
          "timestamp": "2024-01-01T12:00:00.000Z",
          "version": "2.0.0"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.checks.database": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.checks.redis": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.status": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.timestamp": {
              "combine": "AND",
              "matchers": [
                {
                  "format": "yyyy-MM-dd'T'HH:mm:ss.SSSX",
                  "match": "datetime"
                }
              ]
            },
            "$.version": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          }
        },
        "status": 200
      }
    },
    {
      "description": "a request for health status with DB down",
      "providerStates": [
        {
          "name": "database is unavailable"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/api/health"
      },
      "response": {
