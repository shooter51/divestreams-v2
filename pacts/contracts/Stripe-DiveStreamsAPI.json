{
  "consumer": {
    "name": "Stripe"
  },
  "interactions": [
    {
      "description": "a subscription created webhook",
      "providerStates": [
        {
          "name": "valid Stripe signature"
        }
      ],
      "request": {
        "body": {
          "created": 1234567890,
          "data": {
            "object": {
              "customer": "cus_1234567890",
              "id": "sub_1234567890",
              "items": {
                "data": [
                  {
                    "price": {
                      "id": "price_1234567890"
                    }
                  }
                ]
              },
              "status": "active"
            }
          },
          "id": "evt_1234567890",
          "type": "customer.subscription.created"
        },
        "headers": {
          "Content-Type": "application/json",
          "stripe-signature": "t=1234567890, v1=signature"
        },
        "matchingRules": {
          "body": {
            "$.created": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.data.object.customer": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.data.object.id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.data.object.items.data": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type",
                  "min": 1
                }
              ]
            },
            "$.data.object.items.data[*].price.id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.data.object.status": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.type": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          },
          "header": {
            "stripe-signature": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          }
        },
        "method": "POST",
        "path": "/api/webhooks/stripe"
      },
      "response": {
        "body": {
          "received": true
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.received": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          }
        },
        "status": 200
      }
    },
